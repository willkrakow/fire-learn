{
  "compilerOptions": {
    "typeRoots": [
      "./types",
      "./src/types/*",
      "./node_modules/@types",
    ],
    "target": "es5",
    "lib": [
      "dom",
      "DOM.Iterable",
      "esnext"
    ],
    "sourceMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": "."
  },
  "baseUrl": ".",
  "include": [
    "types/**/*",
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
  ],
  "paths": {
    "react": [
      "../node_modules/react"
    ],
    "react-dom": [
      "../node_modules/react-dom/profiling"
    ],
  }
}
